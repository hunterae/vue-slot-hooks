{
  "name": "vue-slot-hooks",
  "version": "0.0.4",
  "main": "dist/vue-slot-hooks.common.js",
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build-bundle": "vue-cli-service build --target lib --name vue-slot-hooks ./src/plugin.js",
    "lint": "vue-cli-service lint --fix"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  },
  "peerDependencies": {
    "vue": ">=2.1.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
